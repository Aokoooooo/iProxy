{
  "name": "LightProxy",
  "version": "1.1.2",
  "license": "MIT",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "npm run lint && yarn compile && npm run prune && CSC_IDENTITY_AUTO_DISCOVERY=false electron-builder --mac --win --x64 && yarn install",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "lint": "npx eslint ./src/**/*.ts ./src/**/*.tsx --fix",
    "update:whistle": "rm -rf whistle && git clone https://github.com/avwo/whistle --depth=1 && patch -u -b whistle/lib/inspectors/res.js -i whistle.patch && yarn add ./whistle ./whistle-start",
    "cleanbuild": "rm -rf node_modules && yarn install && npm run update:whistle && npm run dist",
    "doc": "env SKIP_CACHE=1 bash scripts/depoly-doc.sh",
    "install-deps": "yarn run update:whistle && yarn install",
    "prune": "yarn install --production --ignore-scripts --prefer-offline"
  },
  "electronWebpack": {
    "title": "LightProxy",
    "renderer": {
      "webpackConfig": "webpack/render.additions.webpack.js"
    },
    "main": {
      "webpackConfig": "webpack/main.additions.webpack.js"
    }
  },
  "dependencies": {
    "@sentry/electron": "1.1.0",
    "@sentry/node": "5.10.2",
    "bufferutil": "^4.0.1",
    "cross-spawn": "^7.0.1",
    "electron-better-ipc": "^0.7.0",
    "electron-is-dev": "^1.1.0",
    "electron-log": "^4.0.0",
    "electron-promise-ipc": "^2.0.1",
    "electron-store": "^5.1.0",
    "exit-hook": "^2.2.0",
    "fs-extra-promise": "^1.0.1",
    "get-port": "^5.0.0",
    "global-proxy": "^0.0.4",
    "i18next": "^19.0.2",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "md5-file": "^4.0.0",
    "node-downloader-helper": "^1.0.11",
    "node-fetch": "^2.6.0",
    "qrcode.react": "^1.0.0",
    "source-map-support": "^0.5.12",
    "sudo-prompt": "^9.1.1",
    "tempdir": "^2.0.0",
    "tree-kill": "^1.2.2",
    "universal-analytics": "^0.4.20",
    "whistle": "./whistle",
    "whistle-start": "./whistle-start",
    "whistle.scriptfile": "0.3.3",
    "whistle.vase": "^1.2.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@timkendrick/monaco-editor": "^0.0.9",
    "@types/classnames": "^2.2.9",
    "@types/cross-spawn": "^6.0.1",
    "@types/event-emitter": "^0.3.3",
    "@types/fs-extra-promise": "^1.0.8",
    "@types/ip": "^1.1.0",
    "@types/koa": "^2.11.0",
    "@types/koa-static": "^4.0.1",
    "@types/lodash": "^4.14.149",
    "@types/md5-file": "^4.0.0",
    "@types/node-downloader-helper": "^1.0.0",
    "@types/node-fetch": "^2.5.4",
    "@types/react": "^16.9.16",
    "@types/react-beautiful-dnd": "^11.0.3",
    "@types/react-dom": "^16.9.4",
    "@types/universal-analytics": "^0.4.3",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "antd": "^3.26.6",
    "babel-plugin-import": "^1.13.0",
    "classnames": "^2.2.6",
    "copy-webpack-plugin": "^5.1.1",
    "electron": "^8.0.0-beta.5",
    "electron-builder": "22",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^5.0.1",
    "electron-webpack-ts": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "event-emitter": "^0.3.5",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-event-emitter": "^0.9.1",
    "monaco": "^1.201704190613.0+9ac64297a3b2ace5240299ba54b03f5029378397",
    "monaco-editor": "^0.18.1",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "node-gzip": "^1.1.2",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-i18next": "^11.2.6",
    "react-monaco-editor": "^0.32.1",
    "react-titlebar-osx": "^1.1.0",
    "reset-css": "^5.0.1",
    "shelljs": "^0.8.3",
    "style-loader": "^1.0.2",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2",
    "webpack-build-notifier": "^2.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "build": {
    "appId": "io.github.alibaba.lightproxy",
    "mac": {
      "category": ""
    },
    "win": {
      "target": "nsis",
      "icon": "./static/icon.png"
    }
  }
}
